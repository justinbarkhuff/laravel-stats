{
    "name": "justinbarkhuff/laravel-stats",
    "description": "A fork of spatie/laravel-stats which is backwards compatible with PHP 7.4+ and MySQL 5.7",
    "keywords": [
        "spatie",
        "laravel-stats"
    ],
    "homepage": "https://github.com/justinbarkhuff/laravel-stats",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Barkhuff",
            "email": "justin@justinbarkhuff.com",
            "homepage": "https://justinbarkhuff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-package-tools": "^1.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Stats\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Stats\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Stats\\StatsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
